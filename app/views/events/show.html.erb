<% content_for(:title) do %>
  <%= @event.name %>
<% end %>
<div class="info-link">
  <% if @preview %> 
    <%= link_to 'Back', categories_path, style: "color: #fff"  %>
  <% end %>
</div>
<div class="display">
  <%= render partial: "themes/#{@event.theme_name}", locals: { event: @event } %>
</div>
