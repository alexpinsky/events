<div class="row">
  <div class="small-5 columns">
    <div class="row" id="editor">
      <div class="small-11 columns">
        <%= render partial: "form", locals: { user: user, event: event } %>
      </div>
    </div>
  </div>
  <div class="small-7 columns" id="preview" style="background-image: url(<%= background_collection_url %>)" data-user="<%= user.id %>" data-event="<%= event.id %>">
    <%= render partial: "themes/#{theme}", locals: { event: event } %>
  </div>
</div>