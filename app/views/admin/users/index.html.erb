<div class='admin-wrapper admin-users'>
  <div class='table-wrapper'>
    <table>
      <thead>
        <tr>
          <th>#</th>
          <th>ID</th>
          <th>EMAIL</th>
          <th>Created At</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each_with_index do |user, i| %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.created_at.strftime("%d/%m/%Y") %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>